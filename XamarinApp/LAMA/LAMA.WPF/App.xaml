<Application x:Class="LAMA.WPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:colors="clr-namespace:LAMA.Themes;assembly=LAMA"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:LAMA.WPF"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <Color x:Key="Primary" >#2196F3</Color>

        <!-- Default Command Bar Color -->
        <SolidColorBrush x:Key="CommandBarTextColor" Color="white" />

        <!-- Default Title Bar Color -->
        <SolidColorBrush x:Key="DefaultTitleBarBackgroundColor" Color="white" />
        <SolidColorBrush x:Key="DefaultTitleBarTextColor" Color="white" />

        <!-- Default Tabbed Bar Color -->
        <SolidColorBrush x:Key="DefaultTabbedBarBackgroundColor" Color="white" />
        <SolidColorBrush x:Key="DefaultTabbedBarTextColor" Color="white" />

    </Application.Resources>
</Application>
